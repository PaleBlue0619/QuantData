{ // 注 -> 这里均为createTime
  "stockInfo":{ // 股票基础信息
    "dbName": "dfs://stockDay",
    "tbName": "stockInfo",
    "isTSDB": false,
    "isInfo": true, // 是否是静态信息的维度表 -> false则跳过日志的记录,每次均重新删除后再进行拉取
    "dateCol": null,
    "indicator": {
      "ts_code": "SYMBOL",
      "symbol": "SYMBOL",
      "name": "STRING",
      "area": "STRING",
      "industry": "STRING",
      "market": "SYMBOL",
      "exchange": "SYMBOL",
      "currType": "STRING",
      "listStatus": "STRING",
      "listDate": "DATE",
      "delistDate": "DATE"
    },
    "addCreateTime": true,
    "partitionCol": ["createTime"],
    "sortCol": [],
    "keepDuplicates": "ALL"
  },

  "stockDisclosure": { // 财报披露时间
    "dbName": "dfs://stockDay",
    "tbName": "stockDisclosure",
    "isTSDB": false,
    "isInfo": false, // 是否是静态信息的维度表
    "dateCol": "endDate",
    "indicator": {
      "ts_code": "SYMBOL",
      "annDate": "DATE",
      "endDate": "DATE",
      "preDate": "DATE",
      "actualDate": "DATE",
      "modifyDate": "STRING"
    },
    "addCreateTime": true,
    "partitionCol": ["endDate"],
    "sortCol": [],
    "keepDuplicates": "ALL"
  },

  "stockDailyBasic": { // 每日财务指标
    "dbName": "dfs://stockDay",
    "tbName": "stockDailyBasic",
    "isTSDB": false,
    "isInfo": false, // 是否是静态信息的维度表
    "dateCol": "tradeDate",
    "indicator": {
      "ts_code": "SYMBOL",
      "tradeDate": "DATE",
      "close": "DOUBLE",
      "turnoverRate": "DOUBLE",
      "turnoverRateF": "DOUBLE",
      "volumeRatio": "DOUBLE",
      "PE": "DOUBLE",
      "PE_TTM": "DOUBLE",
      "PB": "DOUBLE",
      "PS": "DOUBLE",
      "PS_TTM": "DOUBLE",
      "DV": "DOUBLE",
      "DV_TTM": "DOUBLE",
      "totalShare": "DOUBLE",
      "floatShare": "DOUBLE",
      "freeShare": "DOUBLE",
      "totalMV": "DOUBLE",
      "circMV": "DOUBLE"
    },
    "addCreateTime": true,
    "partitionCol": ["tradeDate"],
    "sortCol": ["ts_code","tradeDate"], // 这里不会触发 -> 当且仅当isTSDB=false + isInfo = false的时候用作巡检字段 -> 自动删除重复值
    "keepDuplicates": "LAST"
  },

  "stockDailyBar": {
    "dbName": "dfs://stockDay",
    "tbName": "stockDailyBar",
    "isTSDB": false,
    "isInfo": false, // 是否是静态信息的维度表
    "dateCol": "tradeDate",
    "indicator": {
      "ts_code": "SYMBOL",
      "tradeDate": "DATE",
      "open": "DOUBLE",
      "high": "DOUBLE",
      "low": "DOUBLE",
      "close": "DOUBLE",
      "preClose": "DOUBLE",
      "change": "DOUBLE",
      "pctChg": "DOUBLE",
      "volume": "LONG",
      "amount": "DOUBLE",
      "adjFactor": "DOUBLE"
    },
    "addCreateTime": true,
    "partitionCol": ["tradeDate"],
    "sortCol": [],
    "keepDuplicates": "ALL"
  }
}